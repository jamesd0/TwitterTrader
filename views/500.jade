!!! 5
html(lang='en')
//
  <![endif]
  //
     BEGIN HEAD 
  head
    meta(charset='utf-8')
    title Metronic | Extra - 500 Page
    meta(content='width=device-width, initial-scale=1.0', name='viewport')
    meta(content='', name='description')
    meta(content='', name='author')
    //
       BEGIN GLOBAL MANDATORY STYLES 
    link(href='plugins/bootstrap/css/bootstrap.min.css', rel='stylesheet', type='text/css')
    link(href='plugins/bootstrap/css/bootstrap-responsive.min.css', rel='stylesheet', type='text/css')
    link(href='plugins/font-awesome/css/font-awesome.min.css', rel='stylesheet', type='text/css')
    link(href='css/style-metro.css', rel='stylesheet', type='text/css')
    link(href='css/style.css', rel='stylesheet', type='text/css')
    link(href='css/style-responsive.css', rel='stylesheet', type='text/css')
    link#style_color(href='css/themes/default.css', rel='stylesheet', type='text/css')
    link(href='plugins/uniform/css/uniform.default.css', rel='stylesheet', type='text/css')
    //
       END GLOBAL MANDATORY STYLES 
    //
       BEGIN PAGE LEVEL STYLES 
    link(href='css/pages/error.css', rel='stylesheet', type='text/css')
    //
       END PAGE LEVEL STYLES 
    link(rel='shortcut icon', href='favicon.ico')
  //
     END HEAD 
  //
     BEGIN BODY 
  body.fixed-top
    //
       BEGIN HEADER 
    .header.navbar.navbar-inverse.navbar-fixed-top
      //
         BEGIN TOP NAVIGATION BAR 
      .navbar-inner
        .container-fluid
          //
             BEGIN LOGO 
          a.brand(href='index')
            img(src='img/logo.png', alt='logo')
          //
             END LOGO 
          //
             BEGIN RESPONSIVE MENU TOGGLER 
          a.btn-navbar.collapsed(href='javascript:;', data-toggle='collapse', data-target='.nav-collapse')
            img(src='img/menu-toggler.png', alt='')
          //
             END RESPONSIVE MENU TOGGLER 
          //
             BEGIN TOP NAVIGATION MENU 
          ul.nav.pull-right
          //
             END TOP NAVIGATION MENU 
      //
         END TOP NAVIGATION BAR 
    //
       END HEADER 
    //
       BEGIN CONTAINER 
    .page-container.row-fluid
      //
         BEGIN SIDEBAR 
      .page-sidebar.nav-collapse.collapse
        //
           BEGIN SIDEBAR MENU 
        ul
          li
            //
               BEGIN SIDEBAR TOGGLER BUTTON 
            .sidebar-toggler.hidden-phone
            //
               BEGIN SIDEBAR TOGGLER BUTTON 
          li
          li.start
            a(href='index.html')
              i.icon-home
              span.title Dashboard
        //
           END SIDEBAR MENU 
      //
         END SIDEBAR 
      //
         BEGIN PAGE 
      .page-content
        //
           BEGIN SAMPLE PORTLET CONFIGURATION MODAL FORM
        #portlet-config.modal.hide
          .modal-header
            button.close(data-dismiss='modal', type='button')
            h3 Widget Settings
            p Here will be a configuration form
        //
           END SAMPLE PORTLET CONFIGURATION MODAL FORM
        .container-fluid
          //
             BEGIN PAGE HEADER
          .row-fluid
            .span12
              //
                 BEGIN PAGE TITLE & BREADCRUMB
              h3.page-title
                | 500 Page
                small an error has occured
              ul.breadcrumb
                li
                  i.icon-home
                  a(href='index') Home
                  i.icon-angle-right
                li
                  a(href='#') 500 Page
              //
                 END PAGE TITLE & BREADCRUMB
          //
             END PAGE HEADER
          //
             BEGIN PAGE CONTENT
          .row-fluid
            .span12
              .row-fluid.page-500
                .span5.number
                  | 500
                .span7.details
                  h3 Opps, Something went wrong.
                  p
                    | We are fixing it!
                    br
                    | Please come back in a while.
                    br
          //
             END PAGE CONTENT
        //
           END PAGE CONTAINER
      //
         BEGIN PAGE 
    //
       END CONTAINER 
    //
       BEGIN JAVASCRIPTS(Load javascripts at bottom, this will reduce page load time) 
    //
       BEGIN CORE PLUGINS 
    script(src='plugins/jquery-1.8.3.min.js', type='text/javascript')
    //
       IMPORTANT! Load jquery-ui-1.10.1.custom.min.js before bootstrap.min.js to fix bootstrap tooltip conflict with jquery ui tooltip 
    script(src='plugins/jquery-ui/jquery-ui-1.10.1.custom.min.js', type='text/javascript')
    script(src='plugins/bootstrap/js/bootstrap.min.js', type='text/javascript')
    //if lt IE 9
      script(src='plugins/excanvas.js')
      script(src='plugins/respond.js')
    script(src='plugins/breakpoints/breakpoints.js', type='text/javascript')
    //
       IMPORTANT! jquery.slimscroll.min.js depends on jquery-ui-1.10.1.custom.min.js 
    script(src='plugins/jquery-slimscroll/jquery.slimscroll.min.js', type='text/javascript')
    script(src='plugins/jquery.blockui.js', type='text/javascript')
    script(src='plugins/jquery.cookie.js', type='text/javascript')
    script(src='plugins/uniform/jquery.uniform.min.js', type='text/javascript')
    //
       END CORE PLUGINS 
    script(src='scripts/app.js')
    script.
      jQuery(document).ready(function() {
      App.init();
      });
    //
       END JAVASCRIPTS 
  //
     END BODY